# System Configuration
FLASK_ENV=development
FLASK_DEBUG=true
SECRET_KEY=agentic-ai-indonesia-dev-key-2024

# Database
DATABASE_URL=sqlite:///data/agentic.db
CREDENTIAL_DB_PATH=data/credentials.db
CREDENTIAL_MASTER_PASSWORD=agentic_ai_indonesia_secure_2024

# Basic Settings
AGENTIC_SYSTEM_NAME=Agentic AI System
AGENTIC_VERSION=1.0.0
